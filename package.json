{
  "name": "@tsingwong/pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@8.5.0",
  "description": "",
  "keywords": [],
  "author": "tsingwong<tsongwoong@163.com>",
  "homepage": "https://github.com/tsingwong/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsingwong/pkg-name.git"
  },
  "bugs": "https://github.com/tsingwong/pkg-name/issues",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "start": "nodemon --exec ts-node --experimentalSpecifierResolution node src/index.ts --project ./tsconfig.json",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.6",
    "lint-staged": "^13.3.0",
    "nodemon": "^3.0.1",
    "rome": "^12.1.3",
    "simple-git-hooks": "^2.9.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "pnpm rome ci --colors force"
  },
  "publishConfig": {
    "access": "public"
  }
}
